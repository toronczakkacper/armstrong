def armstrong(liczba):
    zamiana = len(str(liczba))
    suma = 0
    temp = liczba
    while temp > 0:
        digit = temp % 10  #digit -> np liczba 153 digit to 1, 5, 3
        suma += digit ** zamiana
        temp //= 10   # "odcinamy" koncowke liczby np 193 -> 19
    if liczba == suma:
        print(liczba, "armstrong number")
    else:
        print(liczba, "not armstrong number")


